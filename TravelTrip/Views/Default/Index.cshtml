@using TravelTrip.Models.Sınıflar
@model List<Blog>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}
<!--TRAVEL AROUND WORLD-->
<div class="fh5co-hero">
    <div class="fh5co-overlay"></div>
    <div class="fh5co-cover text-center" data-stellar-background-ratio="0.5" style="background-image: url(/web/images/pamukkale.png);">
        <div class="desc animate-box">
            <h2>Travel Around The World.</h2>
            <!-- <span>Lovely Crafted by <a href="http://frehtml5.co/" target="_blank" class="fh5co-site-name">FREEHTML5.co</a></span> -->
            <span><a class="btn btn-primary btn-lg" href="#">Get Started</a></span>
        </div>
    </div>

</div>

<!--6 SQUARE CITIES PHOTOS-->
<div class="fh5co-listing">
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-sm-4 fh5co-item-wrap">
                <a class="fh5co-listing-item">
                    <img src="~/web/images/paris.jpg" alt="Free HTML5 Bootstrap Template by FreeHTML5.co" class="img-responsive">
                    <div class="fh5co-listing-copy">
                        <h2>Paris</h2>
                        <span class="icon">
                            <i class="icon-chevron-right"></i>
                        </span>
                    </div>
                </a>
            </div>
            <div class="col-md-4 col-sm-4 fh5co-item-wrap">
                <a class="fh5co-listing-item">
                    <img src="~/web/images/budapest.jpg" alt="Free HTML5 Bootstrap Template by FreeHTML5.co" class="img-responsive">
                    <div class="fh5co-listing-copy">
                        <h2>Budapest</h2>
                        <span class="icon">
                            <i class="icon-chevron-right"></i>
                        </span>
                    </div>
                </a>
            </div>
            <div class="col-md-4 col-sm-4 fh5co-item-wrap">
                <a class="fh5co-listing-item">
                    <img src="~/web/images/london.jpg" alt="Free HTML5 Bootstrap Template by FreeHTML5.co" class="img-responsive">
                    <div class="fh5co-listing-copy">
                        <h2>London</h2>
                        <span class="icon">
                            <i class="icon-chevron-right"></i>
                        </span>
                    </div>
                </a>
            </div>
            <!-- END 3 row -->

            <div class="col-md-4 col-sm-4 fh5co-item-wrap">
                <a class="fh5co-listing-item">
                    <img src="~/web/images/amsterdam.jpg" alt="Free HTML5 Bootstrap Template by FreeHTML5.co" class="img-responsive">
                    <div class="fh5co-listing-copy">
                        <h2>Amsterdam</h2>
                        <span class="icon">
                            <i class="icon-chevron-right"></i>
                        </span>
                    </div>
                </a>
            </div>
            <div class="col-md-4 col-sm-4 fh5co-item-wrap">
                <a class="fh5co-listing-item">
                    <img src="~/web/images/prag.jpg" alt="Free HTML5 Bootstrap Template by FreeHTML5.co" class="img-responsive">
                    <div class="fh5co-listing-copy">
                        <h2>Prag</h2>
                        <span class="icon">
                            <i class="icon-chevron-right"></i>
                        </span>
                    </div>
                </a>
            </div>
            <div class="col-md-4 col-sm-4 fh5co-item-wrap">
                <a class="fh5co-listing-item">
                    <img  src="~/web/images/roma.jpg" alt="Free HTML5 Bootstrap Template by FreeHTML5.co" class="img-responsive">
                    <div class="fh5co-listing-copy">
                        <h2>Rome</h2>
                        <span class="icon">
                            <i class="icon-chevron-right"></i>
                        </span>
                    </div>
                </a>
            </div>
            <!-- END 3 row -->


        </div>
    </div>
</div>

<!--NEWS-->
<div class="fh5co-section">
    <div class="container">
        <div class="row">
            <div class=" fh5co-news" style="text-align: center; ">
                <h3>Some Informations</h3>
                <ul>
                    <li style="margin-top: 2rem;">
                        <a href="#">
                            <h3>Restaurants</h3>
                        </a>
                    </li>
                    <li style="margin-top: 2rem;">
                        <a href="#">
                            <h3>Things To Do</h3>
                        </a>
                    </li>
                    <li style="margin-top: 2rem;">
                        <a href="#">
                            <h3>Flights</h3>
                        </a>
                    </li>

                </ul>
            </div>

        </div>
        @Html.Action("Partial1", "Default")

        <div class="container" style="margin-top:3rem;">
            <div class="row">
                <div class="col-md-4">
                   @Html.Action("Partial3", "Default")
                </div>

                    <div class="col-md-8">
                        @Html.Action("Partial4","Default")
                    </div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</div>




